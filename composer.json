  "autoload": {
    "psr-4": {
      "Devio\\Support\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Devio\\Support\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Israel Ortu√±o",
      "email": "ai.ortuno@gmail.com"
    }
  ],
  "require": {
